<SettingsScreen>:
    BoxLayout:
        orientation:'horizontal'
        canvas:
            Color:
                rgb: 1,1,1
            Rectangle:
                size: self.size  
        GridLayout:
            cols:1
            row_force_default:True
            row_default_height:60
            size_hint:(.6,.8)
            Label:
                text:'Mercados disponibles:'
                color: (0, 0, 0, 1)
                font_size:'27sp'
        GridLayout:
            cols:2
            row_default_height:40
            size_hint:(1,1)
            Label:
                text:"holi"
                color: (0, 0, 0, 1)
            CheckBox:
                text:"Holi"
                color: (0, 0, 0, 1)
            BoxLayout:
                spacing: 20
                orientation: 'vertical'
                size_hint_x: 0.20
                size_hint_y: 0.10
                canvas:
                    Color:
                        rgb: 1,1,1
                Button:
                    text:"Descargar"
                    color: 0, 0, 0, 1
                    background_color: (1, 1, 1, .2)
                    size_hint_y: 0.05
                    size_hint_x:0.7
                    pos:(.5,.5)
                    font_size: self.width/10
                Button:
                    text: 'Ir al menú'
                    on_press: root.manager.current = 'menu'
                    color: 0, 0, 0, 1
                    background_color: (1, 1, 1, .2)
                    size_hint_y: 0.05
                    size_hint_x:0.7
                    pos:(.3,.2)
                    font_size: self.width/10
